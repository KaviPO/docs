{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "DAS Gateway Documentation",
  "languages": [
    {
      "label": "English",
      "code": "en"
    },
    {
      "label": "日本語",
      "code": "jp"
    }
  ],
  "colors": {
    "primary": "#FFAB31",
    "light": "#FFAB31",
    "dark": "#F18A00"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": {
          "en": "Documentation",
          "jp": "ドキュメント"
        },
        "groups": [
          {
            "group": " ",
            "pages": {
              "en": ["documentation/overview"],
              "jp": ["documentation-jp/overview"]
            }
          },
          {
            "group": {
              "en": "Get Started",
              "jp": "スタートガイド"
            },
            "pages": {
              "en": [
                "documentation/introduction",
                "documentation/createaccount",
                "documentation/checklist",
                "documentation/glossary"
              ],
              "jp": [
                "documentation-jp/introduction",
                "documentation-jp/createaccount",
                "documentation-jp/checklist",
                "documentation-jp/glossary"
              ]
            }
          }
        ]
      },
      {
        "tab": {
          "en": "API Reference",
          "jp": "APIリファレンス"
        },
        "groups": [
          {
            "group": {
              "en": "Getting Started",
              "jp": "入門"
            },
            "pages": {
              "en": [
                "api-reference/en/introduction",
                "api-reference/en/authentication",
                "api-reference/en/merchant-details"
              ],
              "jp": [
                "api-reference/jp/introduction",
                "api-reference/jp/authentication",
                "api-reference/jp/merchant-details"
              ]
            }
          }
        ]
      },
      {
        "tab": {
          "en": "Change Logs",
          "jp": "変更履歴"
        },
        "groups": [
          {
            "group": "Logs",
            "pages": {
              "en": [
                "releases/revisions",
                "releases/apiupdate",
                "releases/releasenotes"
              ],
              "jp": [
                "releases-jp/revisions",
                "releases-jp/apiupdate",
                "releases-jp/releasenotes"
              ]
            }
          }
        ]
      },
      {
        "tab": {
          "en": "Support",
          "jp": "サポート"
        },
        "groups": [
          {
            "group": {
              "en": "Guide",
              "jp": "ガイド"
            },
            "pages": {
              "en": ["support/merchantguide", "support/partnerguide"],
              "jp": ["support-jp/merchantguide", "support-jp/partnerguide"]
            }
          }
        ]
      }
    ]
  },
  "logo": {
    "light": "/logo/logo_light.svg",
    "dark": "/logo/logo_dark.svg"
  },
  "appearance": {
    "default": "light",
    "strict": true
  },
  "navbar": {
    "links": [
      {
        "label": {
          "en": "Create an Account",
          "jp": "アカウントを作成"
        },
        "href": "https://me.paymentoptions.com/onboarding/choose-account"
      }
    ],
    "primary": {
      "type": "button",
      "label": {
        "en": "Sign in",
        "jp": "ログイン"
      },
      "href": "https://me.paymentoptions.com"
    }
  },
  "footer": {
    "socials": {
      "website": "https://www.paymentoptions.com",
      "linkedin": "https://www.linkedin.com/company/payment-options/"
    }
  }
}